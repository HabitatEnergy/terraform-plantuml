{
  "name": "tfpuml",
  "version": "0.2.5",
  "description": "Generate PlantUML Images from a Terraform State file",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "remark": "remark . --output",
    "format": "prettier --write \"modules/**/*.js\" \"tests/**/*.js\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "bin": {
    "tfpuml": "index.js",
    "tfpuml-download-plantuml": "download.js"
  },
  "keywords": [
    "Terraform",
    "PlantUML",
    "AWS"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:fmalk/terraform-plantuml.git"
  },
  "author": "Fernando Piancastelli <fernando@cellide.com>",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.2.0",
    "commander": "^10.0.0",
    "request": "^2.88.2",
    "semver": "^7.3.8"
  },
  "devDependencies": {
    "prettier": "^2.8.3",
    "remark-cli": "^11.0.0",
    "remark-preset-lint-consistent": "^5.1.1",
    "remark-preset-lint-recommended": "^6.1.2"
  },
  "remarkConfig": {
    "settings": {
      "bullet": "*"
    },
    "plugins": [
      "remark-preset-lint-consistent",
      "remark-preset-lint-recommended"
    ]
  }
}
